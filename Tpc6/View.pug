extends layout
block content
    .w3-container
        .w3-card-4
            header.w3-container.w3-teal
                h3 Lista de Musicas
            .w3-container
                table.w3-table-all
                    tr
                        th Titulo
                        th Local
                        th Provincia
                        th Musico
                        th Duração
                    each arq in lista1
